<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
    <RootNamespace>TiraxTech</RootNamespace>
    <Title>Immutable URI</Title>
    <Authors>Ruxo Zheng</Authors>
    <Description>Customizable, Immutable URI record</Description>
    <PackageLicenseUrl>https://mit-license.org/</PackageLicenseUrl>
    <PackageTags>uri</PackageTags>
    <Company>Tirax Tech</Company>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <LangVersion>default</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LanguageExt.Core" Version="4.0.3" />
    <PackageReference Include="Microsoft.Extensions.Primitives" Version="6.0.0" />
    <PackageReference Include="MinVer" Version="2.5.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>
